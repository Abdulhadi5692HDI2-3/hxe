class _System {
    init() {
        this.loaded = true; 
    }
    exit(code) {
        _inbuilt_exit(code);
    }
    run(path) {
        return _inbuilt_binrun(path);
    }
    instr() {
        return _inbuilt_readk();
    }
    wait(secs) {
        _inbuilt_wait(secs);
    }
}

var System = _System();